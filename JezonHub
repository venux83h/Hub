-- Make sure WindUI is loaded
local WindUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/LARKY-WindUI/WindUI/main/Init.lua"))() -- Example URL
local Window = WindUI:Window({
    Title = "JezonHub",
    Theme = "Crimson",
    ToggleKey = Enum.KeyCode.RightShift, -- change toggle key if you want
    Size = UDim2.new(0, 500, 0, 400)
})

-- Tab: Body
local Tab = Window:Tab({
    Title = "Body",
    Icon = "bird",
    Locked = false,
})

